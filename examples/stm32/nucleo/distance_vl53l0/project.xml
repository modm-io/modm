<?xml version='1.0' encoding='UTF-8'?>
<library>
  <!--
    ..ci:
        nucleos = [b for b in boards if b.startswith("nucleo_") and "l432" not in b]
        for board in nucleos:
            new_config = config.replace("/distance_vl53l0", "/distance_vl53l0/" + board) \
                               .replace("nucleo_f031k6", board)
            if "303" in board or "f0" in board:
                new_config = new_config.replace(":i2c:1", ":i2c.bitbang")
            yield (new_config, ["scons"], board)
  -->
  <extends>../../../../src/modm/board/nucleo_f031k6/board.xml</extends>
  <options>
    <option name=":build:build.path">../../../../build/generic/distance_vl53l0</option>
  </options>
  <modules>
    <module>:debug</module>
    <module>:driver:vl53l0</module>
    <module>:platform:gpio</module>
    <module>:platform:i2c:1</module>
    <module>:processing:protothread</module>
    <module>:build:scons</module>
  </modules>
</library>
