#include "../device.hpp"

// this should be able to be generated instead of using Macros for this.
%% for id in driver.instance
#include "adc_interrupt_{{ id }}.hpp"
%% endfor

extern "C" void
ADC_IRQHandler(void)
{

%% for id in driver.instance
	if (xpcc::stm32::AdcInterrupt{{ id }}::getInterruptFlags()) {
		xpcc::stm32::AdcInterrupt{{ id }}::handler();
	}
%% endfor

}
